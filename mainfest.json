{
  "manifest_version": 2,
  "name": "YouTube Comment Manager",
  "version": "1.0",
  "description": "Manage YouTube comments easily.",
  "permissions": ["storage", "activeTab", "https://www.youtube.com/"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "YouTube Comment Manager"
  }
}
